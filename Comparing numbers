#this is a begginer level programme to tell us which number is biggest between user defined numbers.
import time
print('Dear sir, please type any 3 numbers . i will show you which one is biggest.')
time.sleep(1.8)
a = eval(input('Enter your first number:'))
b = eval(input('Enter your second number:'))
c = eval(input('Enter your third number:'))
if b<a and b<c :
    if a>c:
        print(a,' is the biggest one.')
    elif c>a:
        print(c,' is the biggest one.')
    else:
        print(a, 'is your biggest one.')
elif a==c and b>a:
    print(b, 'is the your biggest one.')

if c<a and c<b :
    if a>b:
        print(a,' is the biggest one.')
    elif b>a:
        print(b,' is the biggest one.')
    else:
        print(a, 'is your biggest one.')
elif a==b and c>a:
    print(c, 'is the your biggest one.')

if a<b and a<c :
    if c>b:
        print(c,' is the biggest one.')
    elif b>c:
        print(b,' is the biggest one.')
    else:
        print(b, 'is your biggest one.')
elif b==c and a>b:
    print(a, 'is the your biggest one.')

time.sleep(1.4)
print('Thanks for testing me.')
